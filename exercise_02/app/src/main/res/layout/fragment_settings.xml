<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:backgroundTint="#000000"
    tools:context=".SettingsFragment">

    <ImageView
        android:id="@+id/image_01"
        android:name="com.cs_3013.android."
        android:layout_width="208dp"
        android:layout_height="213dp"
        android:layout_marginEnd="5dp"
        android:background="#000000"
        android:contentDescription="@string/image_01"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.439"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.17"
        app:srcCompat="@drawable/blue"
        tools:src="@drawable/puff"
        tools:srcCompat="@drawable/blue" />

    <ImageView
        android:id="@+id/image_3"
        android:name="com.cs_3013.android."
        android:layout_width="208dp"
        android:layout_height="213dp"
        android:layout_marginEnd="5dp"
        android:background="#000000"
        android:contentDescription="@string/image_01"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.439"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.696"
        app:srcCompat="@drawable/orange"
        tools:src="@drawable/puff"
        tools:srcCompat="@drawable/orange" />

</androidx.constraintlayout.widget.ConstraintLayout>